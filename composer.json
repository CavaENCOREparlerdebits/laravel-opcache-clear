{
    "name": "abenevaut/laravel-opcache-clear",
    "description": "This Laravel package allows you to clear the OPcache of a PHP application running in FPM mode with ease.",
    "type": "package",
    "require": {
        "php": ">=5.5.9",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.0.*",
        "laravel/framework": "5.4.*"
    },
    "keywords":
        [
            "laravel",
            "cache",
            "opcache",
            "reset",
            "clear",
            "purge",
            "fpm",
            "php-fpm"
        ],
    "license": "GPL-3",
    "authors": [
        {
            "name": "Antoine Benevaut",
            "email": "antoine@benevaut.fr"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "ABENEVAUT\\Opcache\\Clear\\": "src"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ]
    }
}
